{
  "name": "drupalkit",
  "version": "0.0.0-development",
  "author": "Christian Foidl <christian@wunderwerk.io> (https://www.wunderwerk.io)",
  "keywords": [
    "api",
    "drupal",
    "drupalkit"
  ],
  "license": "ISC",
  "private": true,
  "devDependencies": {
    "@drupalkit/config-prettier": "workspace:0.0.0-development",
    "syncpack": "^9.8.4",
    "turbo": "^1.8.3"
  },
  "scripts": {
    "build": "turbo run build",
    "check-all": "turbo run build lint test typecheck",
    "format:check": "prettier --check 'packages/*/**/*.{ts,tsx}'",
    "format:write": "prettier --write 'packages/*/**/*.{ts,tsx}'",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "typecheck": "turbo run typecheck"
  },
  "packageManager": "pnpm@7.26.3"
}
