{
  "name": "@drupal-kit/core",
  "version": "0.3.1",
  "type": "module",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@wunderwerk/ts-functional": "1.0.0-beta.2",
    "before-after-hook": "^3.0.2",
    "is-plain-object": "^5.0.0",
    "node-fetch": "^3.3.1",
    "qs": "^6.11.1"
  },
  "devDependencies": {
    "@drupal-kit/config-typescript": "workspace:0.3.1",
    "@drupal-kit/eslint-config": "workspace:0.3.1",
    "@drupal-kit/types": "workspace:0.3.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@swc/core": "^1.3.51",
    "@types/node-fetch": "^2.6.3",
    "@types/qs": "^6.9.7",
    "@wunderwerk/node-pkg-version-export": "1.0.0-beta.1",
    "ava": "^5.2.0",
    "esbuild": "^0.17.17",
    "msw": "^1.2.1",
    "rollup": "^3.20.4",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "scripts": {
    "build": "rollup -c",
    "export-version": "node-pkg-version-export src/version.ts",
    "lint": "eslint --ext .ts,.tsx src --max-warnings 0",
    "test": "NODE_NO_WARNINGS=1 ava",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "ava": {
    "files": [
      "tests/**/*.test.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader",
      "ts-node/esm"
    ]
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ]
}
